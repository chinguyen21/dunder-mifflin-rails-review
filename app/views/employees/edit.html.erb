
<h1>Employee Form</h1>
<%= form_for(@employee) do |f| %>
  <label>First name:</label><br>
  <%= f.text_field :first_name %><br>

  <label>Last name:</label><br>
  <%= f.text_area :last_name %><br>

  <label>Alias:</label><br>
  <%= f.text_area :alias %><br>

  <label>Office:</label><br>
  <%= f.text_area :office %><br>
  
  <label>Title:</label><br>
  <%= f.text_area :title %><br>

  <label>Dog:</label><br>
  <%= f.text_area :dog %><br>

  <label>Images Link:</label><br>
  <%= f.text_area :img_url %><br>


  <%= f.submit %>
<% end %>